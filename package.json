{
  "name": "pagarme-payment-service-provider",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "dependencies": {
    "bcryptjs": "2.4.3",
    "cors": "2.8.5",
    "cross-env": "^6.0.3",
    "dotenv": "8.1.0",
    "express": "4.17.1",
    "joi": "14.3.1",
    "morgan": "1.9.1",
    "mysql2": "2.0.1",
    "sequelize": "5.21.2",
    "uuid": "3.3.3"
  },
  "devDependencies": {
    "aglio": "2.3.0",
    "chai": "4.2.0",
    "coveralls": "3.0.9",
    "eslint": "6.3.0",
    "eslint-config-pagarme-base": "3.0.0",
    "eslint-plugin-import": "2.18.2",
    "mocha": "6.2.0",
    "mocha-lcov-reporter": "1.3.0",
    "node-mocks-http": "^1.8.1",
    "nodemon": "1.19.2",
    "nyc": "15.0.0",
    "sequelize-cli": "5.5.1"
  },
  "scripts": {
    "coverage": "nyc report --reporter=lcovonly && cat ./coverage/lcov.info | coveralls",
    "docs:generate": "aglio -i docs/src/index.apib --theme-template triple -o docs/build/output.html",
    "docs:preview": "aglio -i docs/src/index.apib --theme-template triple -s --port 3006",
    "lint": "eslint --config .eslintrc './**/*.js'",
    "start": "nodemon server.js",
    "test": "nyc --reporter=html --reporter=text mocha 'test/**/*.js'",
    "migrate": "npx sequelize db:migrate",
    "seed": "npx sequelize-cli db:seed:all",
    "test:ci": "cross-env NODE_ENV=test",
    "migrate:ci": "cross-env NODE_ENV=test npx sequelize db:migrate",
    "seed:ci": "cross-env NODE_ENV=test npx sequelize-cli db:seed:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/allangrds/pagarme-payment-service-provider.git"
